{
  "compilerOptions": {
    "module": "CommonJS",
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2017",
    "esModuleInterop": true
  },
  "compileOnSave": true,
  "ts-node": {
    "compilerOptions": {
      "noImplicitAny": false
    }
  }
}
